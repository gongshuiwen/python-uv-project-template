{
    "typeCheckingMode": "recommended",
    "pythonVersion": "3.13",
    "venvPath": ".",
    "venv": ".venv",
    "include": [
        "src",
        "test"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "src/experimental",
        "src/typestubs"
    ],
    "ignore": [
        "src/oldstuff"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "stubPath": "src/typestubs",
    "reportMissingImports": "error",
    "reportMissingTypeStubs": false,
}
